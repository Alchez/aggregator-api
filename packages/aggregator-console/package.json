{
  "name": "aggregator-console",
  "version": "0.0.0",
  "scripts": {
    "format": "prettier --write \"./src/**/*.ts\" \"./server/**/*.ts\" \"./test/**/*.ts\" \"./e2e/**/*.ts\"",
    "format:check": "prettier-check \"./src/**/*.ts\" \"./server/**/*.ts\" \"./test/**/*.ts\" \"./e2e/**/*.ts\"",
    "ng": "ng",
    "start:client": "ng serve --proxy-config ./proxy.conf.json -o",
    "build": "ng build",
    "test:client": "ng test --no-progress --watch=false",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "e2e": "ng e2e",
    "server:build": "tsc -p server/tsconfig.json",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:server": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && ng build && tsc -p server/tsconfig.json",
    "start:prod": "node dist/server/main.js",
    "test:server": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@nestjs/common": "^5.6.2",
    "@nestjs/core": "^5.6.2",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "angular-oauth2-oidc": "^5.0.2",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "core-js": "^2.6.3",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "joi": "^14.3.1",
    "mongodb": "^3.1.13",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "~6.3.3",
    "typeorm": "^0.2.12",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@nestjs/testing": "^5.6.2",
    "@types/express": "^4.16.1",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.19",
    "@types/supertest": "^2.0.5",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.16.1",
    "prettier-check": "^2.0.0",
    "protractor": "~5.4.0",
    "supertest": "^3.4.2",
    "ts-jest": "^23.1.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.5.0",
    "tslint": "~5.12.0",
    "typescript": "~3.1.6"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleDirectories": [
      "server",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testRegex": "/server/.*\\.(test|spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  }
}
